{
  "name": "insa_planning",
  "version": "2.0.0",
  "description": "A typescript discord bot",
  "main": "dist/server.js",
  "scripts": {
    "transpile": "tsc",
    "build": "npm install --include=dev && npm run transpile && npm install --only=production && cp package*.json dist && sed 's+dist/server.js+server.js+g' package.json > dist/package.json",
    "start": "npm install --include=dev && npm run transpile && node dist/server.js",
    "dev": "npm install --include=dev && nodemon --config nodemon.json src/server.ts --trace-warnings",
    "dev:debug": "nodemon --config nodemon.json --inspect src/server.ts --trace-warnings"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Apokalypt/INSA_Planning.git"
  },
  "author": "Apokalypt",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Apokalypt/INSA_Planning/issues"
  },
  "homepage": "https://github.com/Apokalypt/INSA_Planning#readme",
  "devDependencies": {
    "@types/cron": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "eslint": "^8.29.0",
    "typescript": "^4.9.4",
    "nodemon": "^2.0.20",
    "ts-node": "^10.9.1"
  },
  "dependencies": {
    "cron": "^2.1.0",
    "dayjs": "^1.11.7",
    "discord.js": "^14.7.1",
    "dotenv": "^16.0.3",
    "module-alias": "^2.2.2",
    "puppeteer": "^19.4.0"
  },
  "_moduleAliases": {
    "@models": "./src/models",
    "@services": "./src/services",
    "@errors": "./src/errors",
    "@enums": "./src/enums",
    "@constants": "./src/Constants"
  }
}
